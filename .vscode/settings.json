{
    "editor.rulers": [
    80, 120
    ],
    "python.testing.pytestArgs": [
        "-m",
        "test"
    ],
    "files.exclude": {
        ".venv":true,
        ".pytest_cache": true,
        "**/__pycache__/**":true
    },
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "python.testing.cwd": "${workspaceRoot}"
}